@article{carmona_fifty_2015,
	title = {Fifty years of co-evolution and beyond: integrating co-evolution from molecules to species},
	volume = {24},
	issn = {1365-294X},
	shorttitle = {Fifty years of co-evolution and beyond},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/mec.13389/abstract},
	doi = {10.1111/mec.13389},
	abstract = {Fifty years after Ehrlich and Raven's seminal paper, the idea of co-evolution continues to grow as a key concept in our understanding of organic evolution. This concept has not only provided a compelling synthesis between evolutionary biology and community ecology, but has also inspired research that extends beyond its original scope. In this article, we identify unresolved questions about the co-evolutionary process and advocate for the integration of co-evolutionary research from molecular to interspecific interactions. We address two basic questions: (i) What is co-evolution and how common is it? (ii) What is the unit of co-evolution? Both questions aim to explore the heart of the co-evolutionary process. Despite the claim that co-evolution is ubiquitous, we argue that there is in fact little evidence to support the view that reciprocal natural selection and coadaptation are common in nature. We also challenge the traditional view that co-evolution only occurs between traits of interacting species. Co-evolution has the potential to explain evolutionary processes and patterns that result from intra- and intermolecular biochemical interactions within cells, intergenomic interactions (e.g. nuclear-cytoplasmic) within species, as well as intergenomic interactions mediated by phenotypic traits between species. Research that bridges across these levels of organization will help to advance our understanding of the importance of the co-evolutionary processes in shaping the diversity of life on Earth.},
	language = {en},
	number = {21},
	urldate = {2016-04-08},
	journal = {Mol Ecol},
	author = {Carmona, Diego and Fitzpatrick, Connor R. and Johnson, Marc T. J.},
	month = nov,
	year = {2015},
	keywords = {epistasis, escape and radiate, intergenomic coevolution, intragenomic coevolution, local adaptation, molecular coevolution, reciprocal selection},
	pages = {5315--5329},
	file = {Full Text PDF:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/PP5IWU64/Carmona et al. - 2015 - Fifty years of co-evolution and beyond integratin.pdf:application/pdf;Snapshot:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/6P8T7UFP/Carmona et al. - 2015 - Fifty years of co-evolution and beyond integratin.html:text/html}
}

@article{phifer-rixey_adaptive_2012,
	title = {Adaptive {Evolution} and {Effective} {Population} {Size} in {Wild} {House} {Mice}},
	volume = {29},
	issn = {0737-4038},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3457769/},
	doi = {10.1093/molbev/mss105},
	abstract = {Estimates of the proportion of amino acid substitutions that have been fixed by selection (α) vary widely among taxa, ranging from zero in humans to over 50\% in Drosophila. This wide range may reflect differences in the efficacy of selection due to differences in the effective population size (Ne). However, most comparisons have been made among distantly related organisms that differ not only in Ne but also in many other aspects of their biology. Here, we estimate α in three closely related lineages of house mice that have a similar ecology but differ widely in Ne: Mus musculus musculus (Ne ∼ 25,000–120,000), M. m. domesticus (Ne ∼ 58,000–200,000), and M. m. castaneus (Ne ∼ 200,000–733,000). Mice were genotyped using a high-density single nucleotide polymorphism array, and the proportions of replacement and silent mutations within subspecies were compared with those fixed between each subspecies and an outgroup, Mus spretus. There was significant evidence of positive selection in M. m. castaneus, the lineage with the largest Ne, with α estimated to be approximately 40\%. In contrast, estimates of α for M. m. domesticus (α = 13\%) and for M. m. musculus (α = 12 \%) were much smaller. Interestingly, the higher estimate of α for M. m. castaneus appears to reflect not only more adaptive fixations but also more effective purifying selection. These results support the hypothesis that differences in Ne contribute to differences among species in the efficacy of selection.},
	number = {10},
	urldate = {2015-11-20},
	journal = {Mol Biol Evol},
	author = {Phifer-Rixey, Megan and Bonhomme, François and Boursot, Pierre and Churchill, Gary A. and Piálek, Jaroslav and Tucker, Priscilla K. and Nachman, Michael W.},
	month = oct,
	year = {2012},
	pmid = {22490822},
	pmcid = {PMC3457769},
	pages = {2949--2955},
	file = {PubMed Central Full Text PDF:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/9E6RKPUC/Phifer-Rixey et al. - 2012 - Adaptive Evolution and Effective Population Size i.pdf:application/pdf}
}

@phdthesis{kraft_phylogenetic_2015,
	address = {Berlin},
	type = {Bachelor's thesis},
	title = {Phylogenetic analysis of {Eimeria} spp. parasitising house mouse subspecies based on selected genetic markers},
	abstract = {Hybrid zones are popular objects for studying genetic framework of speciation as they form natural laboratories in which recombination between distinct strains occurs. Recent studies greatly enhanced understanding of gene flow and genetic constitution across a hybrid zone of two closely related subspecies Mus musculus musculus in Eastern Europe and Mus musculus domesticus found in Western Europe. Here, a novel approach is pursued by focussing on mechanisms of parasitic speciation in hybrid zones, where parasites are exposed to both original parental and a new reservoir of recombinant host genomes. This work attempts to examine interaction of the coccidian parasite Eimeria spp. and their house mouse subspecies hosts for the first time. Using PCR-based assays of selected nuclear (18S rDNA) and extrachromosomal regions (mitochondrial COI and apicoplast tRNA region), serving as molecular markers, combined with sophisticated phylogenetic
approaches, two major Eimeria lineages were identified. While E.ex.Mmd is almost exclusively harbored in M.m.domesticus, E.ex.Mmm shows a broader host specificity parasitising M.m.musculus and hybrids as well as M.m.domesticus, the latter with lower prevalence. Although single-gene Eimeria isolates cluster in separate monophyletic groups,
multilocus phylogeographic indicates that these strains are not locally restricted and
co-exist in a single sampled locality. Besides these general patterns detected for isolates
based on single-gene trees, clade composition between the investigated markers exhibits
dissimilarities. This finding and the fact that tree topologies slightly differ indicates genes
introgressing between the western (E.ex.Mmd) and eastern (E.ex.Mmm) strain. This work is anticipated to give first glimpse into diversity and genetic structure of the dominant Eimeria spp. infecting particular subspecies hosts and their spatial distribution across the house mouse hybrid zone. In order to validate and differentiate observed patterns, further field studies with greater sampling sizes are required. In order to generate larger data sets of genetic markers from the three genomes, future analysis will also include the use of high-throughput PCR techniques.},
	school = {Humboldt University},
	author = {Kraft, Julia},
	month = oct,
	year = {2015},
	file = {BA_Julia_Kraft.pdf:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/HV2C6H6F/BA_Julia_Kraft.pdf:application/pdf}
}

@article{lee_role_2004,
	title = {A role for immunology in invasion biology},
	volume = {19},
	issn = {0169-5347},
	url = {http://www.sciencedirect.com/science/article/pii/S0169534704002095},
	doi = {10.1016/j.tree.2004.07.012},
	abstract = {Invasive species are of increasing conservation and economic concern, yet mechanisms underlying invasions remain poorly understood. We propose that variation in immune defences might help explain why only some introduced populations become invasive. Introduced species escape many of their native diseases, but also face novel pathogens that can induce costly, and sometimes deadly, immune responses in naïve hosts. Therefore, favouring less resource-demanding and dangerous defence mechanisms and allocating a greater proportion of resources to growth and reproduction should favour invasion. Specifically, we argue that successful invaders should reduce costly systemic inflammatory responses, which are associated with fever and metabolic and behavioural changes, and rely more heavily on less expensive antibody-mediated immunity. Here we provide supporting arguments for this hypothesis and generate predictions that are testable using tools from the growing field of ecological immunology.},
	number = {10},
	urldate = {2016-02-23},
	journal = {Trends in Ecology \& Evolution},
	author = {Lee, Kelly A. and Klasing, Kirk C.},
	month = oct,
	year = {2004},
	pages = {523--529},
	file = {ScienceDirect Full Text PDF:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/RCZ3I7QJ/Lee and Klasing - 2004 - A role for immunology in invasion biology.pdf:application/pdf;ScienceDirect Snapshot:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/MSC4F77J/S0169534704002095.html:text/html}
}

