@article{shaw_patterns_1998,
	title = {Patterns of macroparasite aggregation in wildlife host populations},
	volume = {117},
	issn = {1469-8161, 0031-1820},
	url = {https://www.cambridge.org/core/journals/parasitology/article/patterns-of-macroparasite-aggregation-in-wildlife-host-populations/8FC8D27B7830F00E0A6128C6818F35B0},
	abstract = {Frequency distributions from 49 published wildlife host–macroparasite
systems were analysed by maximum likelihood for
goodness of fit to the negative binomial distribution. In 45 of the 49
(90\%) data-sets, the negative binomial distribution
provided a statistically satisfactory fit. In the other 4 data-sets the
negative binomial distribution still provided a better
fit than the Poisson distribution, and only 1 of the data-sets fitted the
Poisson distribution. The degree of aggregation was
large, with 43 of the 49 data-sets having an estimated k of less
than 1. From these 49 data-sets, 22 subsets of host data
were available (i.e. host data could be divided by either host sex, age,
where or when hosts were sampled). In 11 of these
22 subsets there was significant variation in the degree of aggregation
between host subsets of the same host–parasite
system. A common k estimate was always larger than that obtained
with all the host data considered together. These
results indicate that lumping host data can hide important variations in
aggregation between hosts and can exaggerate the
true degree of aggregation. Wherever possible common k estimates
should be used to estimate the degree of aggregation.
In addition, significant differences in the degree of aggregation between
subgroups of host data, were generally associated
with significant differences in both mean parasite burdens and the prevalence
of infection.},
	number = {6},
	urldate = {2016-09-08},
	journal = {Parasitology},
	author = {Shaw, D. J. and Grenfell, B. T. and Dobson, A. P.},
	month = dec,
	year = {1998},
	keywords = {aggregation, macroparasites, negative binomial, Wildlife},
	pages = {597--610},
	file = {Full Text PDF:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/4XNEFQHS/Shaw et al. - 1998 - Patterns of macroparasite aggregation in wildlife .pdf:application/pdf;Snapshot:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/8CS49DSF/8FC8D27B7830F00E0A6128C6818F35B0.html:text/html}
}

@article{klindworth_evaluation_2013,
	title = {Evaluation of general 16S ribosomal {RNA} gene {PCR} primers for classical and next-generation sequencing-based diversity studies},
	volume = {41},
	issn = {0305-1048},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3592464/},
	doi = {10.1093/nar/gks808},
	abstract = {16S ribosomal RNA gene (rDNA) amplicon analysis remains the standard approach for the cultivation-independent investigation of microbial diversity. The accuracy of these analyses depends strongly on the choice of primers. The overall coverage and phylum spectrum of 175 primers and 512 primer pairs were evaluated in silico with respect to the SILVA 16S/18S rDNA non-redundant reference dataset (SSURef 108 NR). Based on this evaluation a selection of ‘best available’ primer pairs for Bacteria and Archaea for three amplicon size classes (100–400, 400–1000, ≥1000 bp) is provided. The most promising bacterial primer pair (S-D-Bact-0341-b-S-17/S-D-Bact-0785-a-A-21), with an amplicon size of 464 bp, was experimentally evaluated by comparing the taxonomic distribution of the 16S rDNA amplicons with 16S rDNA fragments from directly sequenced metagenomes. The results of this study may be used as a guideline for selecting primer pairs with the best overall coverage and phylum spectrum for specific applications, therefore reducing the bias in PCR-based microbial diversity studies.},
	number = {1},
	urldate = {2016-08-30},
	journal = {Nucleic Acids Res},
	author = {Klindworth, Anna and Pruesse, Elmar and Schweer, Timmy and Peplies, Jörg and Quast, Christian and Horn, Matthias and Glöckner, Frank Oliver},
	month = jan,
	year = {2013},
	pmid = {22933715},
