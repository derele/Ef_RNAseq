@article{chouvardas_inferring_2016,
	title = {Inferring active regulatory networks from gene expression data using a combination of prior knowledge and enrichment analysis},
	volume = {17},
	issn = {1471-2105},
	url = {http://dx.doi.org/10.1186/s12859-016-1040-7},
	doi = {10.1186/s12859-016-1040-7},
	abstract = {Under both physiological and pathological conditions gene expression programs are shaped through the interplay of regulatory proteins and their gene targets, interactions between which form intricate gene regulatory networks (GRN). While the assessment of genome-wide expression for the complete set of genes at a given condition has become rather straight-forward and is performed routinely, we are still far from being able to infer the topology of gene regulation simply by analyzing its “descendant” expression profile. In this work we are trying to overcome the existing limitations for the inference and study of such regulatory networks. We are combining our approach with state-of-the-art gene set enrichment analyses in order to create a tool, called Regulatory Network Enrichment Analysis (RNEA) that will prioritize regulatory and functional characteristics of a genome-wide expression experiment.},
	number = {5},
	urldate = {2016-06-13},
	journal = {BMC Bioinformatics},
	author = {Chouvardas, Panagiotis and Kollias, George and Nikolaou, Christoforos},
	year = {2016},
	keywords = {Gene regulatory network, Gene set enrichment analysis, Transcriptional regulation},
	pages = {319--332},
	file = {Full Text PDF:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/4AHWZ7CB/Chouvardas et al. - 2016 - Inferring active regulatory networks from gene exp.pdf:application/pdf;Snapshot:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/IJDQIXTJ/s12859-016-1040-7.html:text/html}
