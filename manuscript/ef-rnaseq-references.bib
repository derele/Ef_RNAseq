	month = jul,
	year = {2011},
	keywords = {house mouse, hybrid zones, linkage disequilibrium, SNP markers},
	pages = {2985--3000},
	file = {Full Text PDF:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/5FRAQQSJ/Wang et al. - 2011 - Measures of linkage disequilibrium among neighbour.pdf:application/pdf;Snapshot:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/QHP426HB/abstract.html:text/html}
