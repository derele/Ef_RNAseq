	year = {2003},
	file = {Snapshot:/home/totta/.mozilla/firefox/gc3olsg5.default/zotero/storage/MXDU5JQ6/Tangled Trees.html:text/html}
}

@article{litman_reconstructing_2005,
	title = {Reconstructing immune phylogeny: new perspectives},
	volume = {5},
	copyright = {© 2005 Nature Publishing Group},
